						<xpath>Defs/ResearchProjectDef[defName="EA_Research_1"]/researchMods/li[@Class="ExpertiseAugmentation.EA_ResearchMod"]/MechsToAugment</xpath>
						<value>
							<li>AM_Mech_Librarian</li>
						</value>
					</match>
				</li>
				<li Class="PatchOperationFindMod">
					<success>Always</success>
					<mods>
						<li>Vanilla Fishing Expanded</li>
					</mods>
