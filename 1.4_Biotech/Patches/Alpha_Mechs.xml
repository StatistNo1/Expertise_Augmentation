<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<success>Always</success>
		<mods>
			<li>Alpha Mechs</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ResearchProjectDef[defName="EA_Research_1"]/researchMods/li[@Class="ExpertiseAugmentation.EA_ResearchMod"]/MechsToAugment</xpath>
					<value>
						<li>AM_Mech_MasterChef</li>
						<li>AM_PristineStrider</li>
						<li>AM_Mech_TurboCleaner</li>
					</value>
				</li>
				<li Class="PatchOperationFindMod">
					<success>Always</success>
					<mods>
						<li>Alpha Bees</li>
					</mods>
					<match Class="PatchOperationAdd">
						<xpath>Defs/ResearchProjectDef[defName="EA_Research_1"]/researchMods/li[@Class="ExpertiseAugmentation.EA_ResearchMod"]/MechsToAugment</xpath>
						<value>
							<li>AM_Mech_Apiarist</li>
						</value>
					</match>
				</li>
				<li Class="PatchOperationFindMod">
					<success>Always</success>
					<mods>
						<li>Vanilla Genetics Expanded</li>
